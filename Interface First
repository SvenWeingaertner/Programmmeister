package se_03;

public interface KundenIF {
	void setStammkunde();
	boolean getStammkunde();
	double getPrice();
	double getTime();
}

package se_03;

import static org.junit.jupiter.api.Assertions.*;

import org.junit.Assert;
import org.junit.jupiter.api.Test;

class KundenIFTest {
	
	Kunden a,b,c;

	@Test
	void setStammkunde_() {
		fail("Not yet implemented");
	}

	@Test
	void testGetStammkunde() {
		assertTrue(a.getStammkunde());
	}

	@Test
	void testGetPrice() {
		assertEquals(a.getPrice(), 0.75);
	}

	@Test
	void getTime_onePointFiveInput_ReturnsTrue() {
		assertEquals(a.getTime(), 1.5);
	}

}


